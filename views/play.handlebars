<div class="all">
    <div class="teamSelectedCont">
        <h3>Managers Name: Cole</h3>
        <h3>Team Name: Cole's Team</h3>
    </div>
    <div class="threshholdsCont">
        <h2 id="wildcard">WildCard</h2>
        <h2 id="LDS">LDS</h2>
        <h2 id="LCS">LCS</h2>
        <h2 id="WorldSeries">World Series</h2>
        <button id="next">Advance To Next Round</button>
    </div>
    
    <div class="selectedTeamCont">
        <div class="table">
            <h4>Positions </h4>
            <h4>First Name </h4>
            <h4>Last Name </h4>
            <h4>War </h4>
        </div>

        <div class="playerCont">
            {{#each players}}
            <h4 class="player">
                {{position}}
                {{firstName}}
                {{lastName}}
                {{war}}
                {{/each}}
            </h4>

        </div>

    </div>
    <script src="/js/play.js"></script>